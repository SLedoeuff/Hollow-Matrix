 int tab;
    for (int i = 0 ; i < taille ; i ++ )                    // RÃ©alisation d'une transposition de matrice
    {
        for ( int j = i+1 ; j < taille ; j ++ )
        {
            tab = matrice[j][i];
            matrice[j][i] = matrice[i][j];
            matrice[i][j] = tab;
        }
    }
